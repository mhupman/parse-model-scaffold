if typeof window == 'undefined'
    Parse = require('parse').Parse


<%= @class_name %> = Parse.Object.extend "<%= @class_name %>",
    initialize: ->
        @attrs =
            <% @attrs.each do |attr| %>
            <%= attr %>: (val) => if !val then @get '<%= attr %>' else @set '<%= attr %>', val
            <% end %>

if typeof window == 'undefined'
    module.exports = <%= @class_name %>